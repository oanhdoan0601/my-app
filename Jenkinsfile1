node {
  stage('SCM checkout'){
  git 'https://github.com/oanhdoan0601/my-app'
  }
  stage('Comple and Package'){
  sh 'mvn package'
  }
  stage('Email Notification'){
      mail bcc: '', body: '''Hi, 
      Welcome to jenkin email alerts
      Thanks, 
      Cecilia''', cc: '', from: '', replyTo: '', subject: 'jenkin job git-maven', to: 'missdoan0601@gmail.com'
   }  
 }
