node {
  stage('SCM checkout'){
  git 'https://github.com/oanhdoan0601/my-app'
  }
  stage('Comple and Package'){
  sh 'mvn package'
  }
  stage('Email Notification'){
	          mail bcc: '', body: """Hi Team, You build successfully deployed
		                       Job URL : ${env.JOB_URL}
							                      Job Name: ${env.JOB_NAME}
  Thanks,
  mail bcc: '', body: 'hi, check it', cc: '', from: '', replyTo: '', subject: 'jenkinsfile-git-maven', to: 'missdoan0601@gmail.com'
  }
  }
